<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>

<AnchorPane prefWidth="450" prefHeight="350"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.unito.client.Controller.MailDetailsController">

    <Label layoutX="40.0" layoutY="30.0" text="From:" style="-fx-font-size: 14px;"/>
    <Label fx:id="from" layoutX="80.0" layoutY="32.0" />
    <Label layoutX="40.0" layoutY="60.0" text="to:" style="-fx-font-size: 14px;"/>
    <Label fx:id="to" layoutX="65.0" layoutY="62.0" />
    <Label layoutX="40.0" layoutY="90.0" text="Object:" style="-fx-font-size: 14px;"/>
    <Label fx:id="object" layoutX="93.0" layoutY="92.0" />
    <Label layoutX="40.0" layoutY="120.0" text="Content:" style="-fx-font-size: 16px;"/>
    <Label fx:id="content" layoutX="40.0" layoutY="150.0"/>
    <Label fx:id="date" layoutX="50.0" layoutY="300.0"/>

</AnchorPane>